{
  "name": "iskolutions-frontend",
  "version": "1.0.0",
  "description": "Frontend for Iskolutions AWS Solar Hackathon",
  "type": "module",
  "scripts": {
    "dev": "bun --watch src/server.js",
    "build": "bun run build.js",
    "start": "bun src/server.js",
    "docker:build": "docker build -t iskolutions-frontend .",
    "docker:run": "docker run -p 3000:3000 iskolutions-frontend",
    "ecr:deploy": "bun deploy-ecr.js"
  },
  "dependencies": {
    "hono": "^3.12.0"
  },
  "devDependencies": {
    "bun-types": "latest"
  }
}
